	TSTO #0
	BOV stack_overflow_error
	ADDSP #5
	ADDSP #1
	NEW #1, R0
	BOV stack_overflow_error
	LEA 3(GB), R1
	STORE R1, 0(R0)
	PUSH R0
	BSR init.A
	POP R0
	LOAD R0, R1
