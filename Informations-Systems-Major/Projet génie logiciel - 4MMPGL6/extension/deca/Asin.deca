// fonction en deca en Asin
{
// valeur de l'angle doit etre plus petit que 0.4 pour le test
float x;
// les coeff du polynomes
float z5;
float z3;
float z1;

float c2;
float c0;

// variable
float x2;
float result;
float P;
float Q;

// affectation
z5 = 61;
z3 = 1080;
z1 = 2520;

c2 = 1500;
c0 = 2520;

// initialisation

x = 0.2222;

x2 =  x*x ;
P = (z5 * x2 + z3) * x2 - z1;
Q = c2 * x2 - c0;
result = x*(P/Q);
print(result);

}
